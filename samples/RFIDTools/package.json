{
  "name": "rfidtools",
  "version": "1.0.0",
  "description": "A little tool for controlling the Invengo XC1003 RFID reader",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-packager . RFIDTools --platform=win32 --arch=x64 --out=../dist --overwrite",
    "build-mac": "electron-packager . RFIDTools --platform=darwin --arch=x64 --out=../dist --overwrite",
    "build-linux": "electron-packager . RFIDTools --platform=linux --arch=x64 --out=../dist --overwrite",
    "build-all": "electron-packager . RFIDTools --platform=win32,darwin --arch=x64 --out=../dist --overwrite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "NVA",
  "license": "ISC",
  "devDependencies": {
    "electron-prebuilt": "^1.2.1"
  },
  "dependencies": {
    "material-design-lite": "^1.1.3",
	"electron-packager": "7.0.3",
	"md5": "2.1.0",
	"jquery": "2.2.4",
	"sha1": "1.1.1",
	"socket.io": "1.4.6",
	"socket.io-client": "1.4.6"
  }
}
